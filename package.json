{
  "name": "@creditkarma/graphql-validator",
  "version": "0.2.1",
  "description": "Validate queries agains a GraphQL Schema",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf coverage/ dist/ node_modules/",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint src/**/*",
    "pretest": "npm run build",
    "test": "npm run test:only --",
    "test:watch": "npm test -- -w",
    "test:only": "lab  -c -S ./dist/test",
    "coverage": "lab -c -S -r console -o stdout -r html -o ./coverage/coverage.html ./dist/test"
  },
  "bin": {
    "graphql-validator": "./bin/query-validator.js"
  },
  "keywords": [
    "GraphQL"
  ],
  "author": "Credit Karma",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/code": "^4.0.0",
    "@types/glob": "^5.0.30",
    "@types/lab": "^11.1.0",
    "@types/mkdirp": "^0.3.29",
    "@types/node": "^6.0.46",
    "@types/rimraf": "0.0.28",
    "code": "^4.0.0",
    "lab": "^11.2.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4",
    "tslint": "^4.0.2",
    "typed-graphql": "^1.0.2",
    "typescript": "^2.1.0"
  },
  "dependencies": {
    "@creditkarma/graphql-loader": "^0.2.0",
    "commander": "^2.9.0",
    "glob": "^7.0.5",
    "graphql-tools": "^0.8.0"
  },
  "peerDependencies": {
    "graphql": "^0.6.1 || ^0.7.0 || ^0.8.2"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
